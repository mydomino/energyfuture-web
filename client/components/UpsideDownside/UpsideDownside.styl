.upside-downside
  margin-top 20px
  ul
    display inline-block
    width 50%
    padding-left 0
    margin 0
    &:last-child
      float right
      padding-left 5%
      border-left 1px solid rgba(#979797, 0.7)
    h2
      margin-top -12px
      font-variant small-caps
    li
      margin 10px 0 0 20px
      width 90%
